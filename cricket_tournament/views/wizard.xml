<odoo>
	<data>
		 <record model="ir.ui.view" id="wizard_form_view">
            <field name="name">wizard form</field>
            <field name="model">ocsb.wizard</field>
            <field name="arch" type="xml">
                <form string="Add Tournament">
                    <group>
                        <field name="tournament_id"/>
                    </group>
                </form>
            </field>
        </record>

        <act_window id="launch_session_wizard"
                    name="Add Score"
                    binding_model="tournament.detail"
                    res_model="ocsb.wizard"
                    view_mode="form"
                    target="new"/>

        <!-- <menuitem id="sub_menu_wizard_app" name="Wizard" action="launch_session_wizard" parent="main_menu_live_cricket_score_board" sequence="6" /> -->

	</data>
</odoo>

